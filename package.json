{
  "name": "ai-scraper-unified",
  "version": "1.0.0",
  "description": "Aplicaci√≥n unificada de Web Scraper con Backend (Express) y Frontend (Next.js)",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:clean": "rm -rf .next && node server.js",
    "dev:stable": "NODE_OPTIONS='--max-old-space-size=4096' node server.js",
    "build": "next build",
    "build:clean": "rm -rf .next && next build",
    "start": "node server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "health-check": "npm run type-check && npm run lint:fix",
    "clean": "rm -rf .next node_modules/.cache",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:e2e": "playwright test tests/e2e/",
    "test:e2e:mobile": "playwright test tests/e2e/ --project=\"Mobile Chrome\"",
    "test:e2e:tablet": "playwright test tests/e2e/ --project=Tablet",
    "test:performance": "playwright test tests/performance/",
    "test:load": "node tests/load/load-test.js",
    "test:all": "npm run test:e2e && npm run test:performance",
    "test:ci": "playwright test --reporter=json --reporter=junit",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "build:analyze": "ANALYZE=true npm run build",
    "optimize:images": "next-optimized-images"
  },
  "dependencies": {
