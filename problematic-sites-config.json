{
  "tele13radio.cl": {
    "name": "Tele13 Radio",
    "domain": "tele13radio.cl",
    "method": "puppeteer",
    "listingSelectors": {
      "containerSelector": "a[href*=\"/noticia/\"], article, .noticia, [class*=\"news-item\"]",
      "linkSelector": "a",
      "titleSelector": "[title], .title, h2, h3"
    },
    "scrollStrategy": "aggressive",
    "scrollIterations": 15,
    "waitForSelector": "a[href*=\"/noticia/\"]",
    "timeout": 30000
  },
